{
  "name": "postcss-prefixwrap",
  "version": "1.37.0",
  "description": "A PostCSS plugin that is used to wrap css styles with a css selector to constrain their affect on parent elements in a page.",
  "keywords": [
    "css",
    "javascript",
    "nodejs",
    "pnpm",
    "postcss",
    "postcss-plugin",
    "typescript",
    "yarn"
  ],
  "homepage": "https://github.com/dbtedman/postcss-prefixwrap#readme",
  "bugs": "https://github.com/dbtedman/postcss-prefixwrap/issues",
  "repository": "https://github.com/dbtedman/postcss-prefixwrap",
  "license": "MIT",
  "author": {
    "name": "Daniel Tedman",
    "url": "https://danieltedman.com"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "*.md",
    "build"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "clean": "node automation-clean.js",
    "format": "sort-package-json && prettier . --write && pnpm eslint --fix . --ext .js,.ts",
    "lint": "sort-package-json --check && prettier . --check && pnpm eslint . --ext .js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@jest/globals": "29.1.2",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-security-node": "1.1.1",
    "glob": "8.0.3",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "postcss": "8.4.17",
    "postcss7": "npm:postcss@7.0.34",
    "prettier": "2.7.1",
    "sort-package-json": "2.0.0",
    "source-map": "0.7.4",
    "source-map-js": "1.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "postcss": "*"
  }
}
