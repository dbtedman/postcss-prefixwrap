{
  "name": "postcss-prefixwrap",
  "version": "1.40.0",
  "description": "A PostCSS plugin that is used to wrap css styles with a css selector to constrain their affect on parent elements in a page.",
  "keywords": [
    "css",
    "javascript",
    "nodejs",
    "pnpm",
    "postcss",
    "postcss-plugin",
    "typescript",
    "yarn"
  ],
  "homepage": "https://github.com/dbtedman/postcss-prefixwrap#readme",
  "bugs": "https://github.com/dbtedman/postcss-prefixwrap/issues",
  "repository": "https://github.com/dbtedman/postcss-prefixwrap",
  "license": "MIT",
  "author": {
    "name": "Daniel Tedman",
    "url": "https://tedman.dev"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "*.md",
    "build"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.app.json",
    "clean": "node automation-clean.js",
    "format": "sort-package-json && prettier . --write && pnpm eslint --fix . --ext .js,.ts",
    "lint": "sort-package-json --check && prettier . --check && pnpm eslint . --ext .js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@jest/globals": "29.6.2",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.8",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security-node": "1.1.1",
    "glob": "10.3.3",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "postcss": "8.4.27",
    "postcss7": "npm:postcss@7.0.36",
    "prettier": "3.0.1",
    "sort-package-json": "2.5.1",
    "source-map": "0.7.4",
    "source-map-js": "1.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "postcss": "*"
  },
  "pnpm": {
    "overrides": {
      "debug@<3.1.0": ">=3.1.0"
    }
  }
}
